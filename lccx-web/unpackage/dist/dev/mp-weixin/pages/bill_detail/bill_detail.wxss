.ticket {
  background-color: #fff;
  border-radius: 10rpx;
  padding: 40rpx 40rpx 20rpx 40rpx;
  -webkit-box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.03);
          box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.03);
  margin: 0rpx 30rpx 30rpx 30rpx;
}
.ticket .main {
    border-bottom: 1px solid #F1F1F1;
    position: relative;
}
.ticket .main image {
      position: absolute;
      right: 0rpx;
      top: 0rpx;
      width: 168rpx;
}
.ticket .main .title {
      padding-bottom: 20rpx;
      font-size: 34rpx;
      color: #4d4d4d;
      font-weight: bold;
}
.ticket .main .text {
      color: #9F9F9F;
      font-size: 26rpx;
}
.ticket .main .text:last-child {
      margin-bottom: 30rpx;
}
.ticket .link {
    text-align: center;
    color: #3A7FF7;
    font-size: 26rpx;
    padding-top: 20rpx;
}
.ticket .pay_text {
    color: #3A7FF7;
