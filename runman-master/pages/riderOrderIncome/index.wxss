/* pages/riderOrderIncome/index.wxss */
page,.orderIncome{
  width: 100%;
  height: 100%;
  background-color: #FCFCFC;
}
.commonView{
  width: 100%;
  height: 310rpx;
  box-sizing: border-box;
  padding: 90rpx 0 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.contentCommon{
  width:180rpx;
  height: 210rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.imgCommon{
  width: 100rpx;
  height: 100rpx;
}
.titleCommon{
  color: #4A4A4A;
  font-size: 28rpx;
  font-weight: bold;
  max-width: 100%;
}
.valueCommon{
  color: #8E8F90;
  font-size: 28rpx;
  font-weight: normal;
  max-width: 100%;
}
.tixian{
  width: 410rpx;
  height: 100rpx;
  margin: 330rpx auto 0;
  background-color: #F5D200;
  border-radius: 50rpx;
  text-align: center;
  line-height: 100rpx;
  color: #4A4A4A;
  font-size: 28rpx;
  font-weight: normal;
}