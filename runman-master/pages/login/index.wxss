/* pages/login/index.wxss */
page{
  width:100%;
  height:100%;
  background-color: rgb(255, 234, 72)
}

.container{
  display: flex;
  justify-content: center;
  align-items: center;
  width:100%;
  height:100%;
  position: relative;
}

.imgbox{
  width: 100%;
  height: 100%;
}

.imgbox image{
  width:100%;
  /* height:100%; */
  position: absolute;
  bottom: 0
}

.btnbox{
  position: absolute;
  bottom: 0;
  opacity: 0;
}

.content {
  font-size: 32rpx;
  color: #999;
}
.confirm-btn::after{
  border: none;
}
.confirm-btn{
  margin-top: 80rpx;
  height: 300rpx;
  width: 720rpx;
  color: #2E946D;
  background-color: #CEDBBF;
  font-size: 30rpx;
  font-weight: 300;
  line-height: 70rpx;
}
.unauthorized{
  width: 283px;
  height: 283px;
}