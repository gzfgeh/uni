<view><view class="feedback-body feedback-uploader"><view class="uni-uploader"><view class="uni-uploader-body"><view class="uni-uploader__files"><block wx:for="{{imageList}}" wx:for-item="image" wx:for-index="index" wx:key="index"><block><view class="uni-uploader__file" style="position:relative;"><image class="uni-uploader__img" src="{{image}}" data-event-opts="{{[['tap',[['previewImage',[index]]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['close',[index]]]]]}}" class="close-view" bindtap="__e">x</view></view></block></block><view hidden="{{!(imageList.length<8)}}" class="uni-uploader__input-box"><view data-event-opts="{{[['tap',[['chooseImg',['$event']]]]]}}" class="uni-uploader__input" bindtap="__e"></view></view></view></view></view></view><view class="canvasBox"><canvas style="{{'width:'+(imgWidth+'px')+';'+('height:'+(imgHeight+'px')+';')}}" canvas-id="shareCanvas"></canvas></view><view class="bottom"><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" class="row_around" bindchange="__e"><label><radio value="0" checked="{{checked==0}}"></radio>横向拼接</label><label><radio value="1" checked="{{checked==1}}"></radio>竖向拼接</label></radio-group></view></view>