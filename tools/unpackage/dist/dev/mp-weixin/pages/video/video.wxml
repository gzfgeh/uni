<view><uni-status-bar vue-id="1" showBack="{{true}}" titleStr="{{titleStr}}" data-event-opts="{{[['^finishPage',[['backAction']]]]}}" bind:finishPage="__e" bind:__l="__l"></uni-status-bar><scroll-view class="content" style="{{'height:'+(scrollHeightVal+'px')+';'}}" scroll-y="true" scroll-with-animation="true"><image class="pingtai" src="{{pingtai}}" mode="aspectFill"></image><view class="content_wrap"><view class="row btn_wrap"><view class="input_wrap row"><input style="width:100%;" type="text" placeholder="请粘贴视频地址" data-event-opts="{{[['input',[['__set_model',['','videoUrl','$event',[]]]]]]}}" value="{{videoUrl}}" bindinput="__e"/><block wx:if="{{videoUrl}}"><image src="{{clear}}" mode="aspectFill" data-event-opts="{{[['tap',[['clearUrl',['$event']]]]]}}" bindtap="__e"></image></block></view><button type="primary" data-event-opts="{{[['tap',[['getVideoUrl',['$event']]]]]}}" bindtap="__e">提取</button></view><button class="share" type="primary" open-type="share">分享给好友</button><block wx:if="{{trueVideoUrl}}"><video class="video_style" src="{{trueVideoUrl}}" controls></video></block><block wx:if="{{trueVideoUrl}}"><view class="row_around" style="margin-top:30rpx;"><block wx:if="{{false}}"><button type="primary" data-event-opts="{{[['tap',[['saveVideo',['$event']]]]]}}" bindtap="__e">保存到相册</button></block><button type="primary" data-event-opts="{{[['tap',[['copyUrl',['$event']]]]]}}" bindtap="__e">复制视频链接</button></view></block></view></scroll-view></view>