<view><uni-status-bar vue-id="1" showBack="{{true}}" titleStr="{{titleStr}}" data-event-opts="{{[['^finishPage',[['backAction']]]]}}" bind:finishPage="__e" bind:__l="__l"></uni-status-bar><view class="head"><view class="row item"><label class="_span">二维码内容：</label><input type="text" placeholder="请输入二维码内容" data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]]]]]}}" value="{{content}}" bindinput="__e"/></view><view class="row item"><label class="_span">背景色：</label><input type="text" placeholder="请输入背景色" data-event-opts="{{[['input',[['__set_model',['','background','$event',[]]]]]]}}" value="{{background}}" bindinput="__e"/></view><view class="row item"><label class="_span">前景色：</label><input type="text" placeholder="请输入前景色" data-event-opts="{{[['input',[['__set_model',['','foreground','$event',[]]]]]]}}" value="{{foreground}}" bindinput="__e"/></view><view class="row item"><label class="_span">插入logo：</label><block wx:if="{{!icon}}"><image class="upload" src="{{upload}}" mode="aspectFill" data-event-opts="{{[['tap',[['selectImg',['$event']]]]]}}" bindtap="__e"></image></block><block wx:if="{{icon}}"><image class="upload" src="{{icon}}" mode="aspectFill" data-event-opts="{{[['tap',[['selectImg',['$event']]]]]}}" bindtap="__e"></image></block></view><button type="primary" data-event-opts="{{[['tap',[['productImg',['$event']]]]]}}" bindtap="__e">生成二维码</button><block wx:if="{{val}}"><image class="erweima" src="{{src}}" mode="aspectFill"></image></block><block wx:if="{{val}}"><button type="primary" data-event-opts="{{[['tap',[['saveImg',['$event']]]]]}}" bindtap="__e">保存到相册</button></block></view><view data-event-opts="{{[['longpress',[['saveQrcode',['$event']]]]]}}" style="visibility:hidden;margin-top:-440rpx;" bindlongpress="__e"><tki-qrcode class="vue-ref" vue-id="2" val="{{val}}" size="{{size}}" unit="{{unit}}" background="{{background}}" foreground="{{foreground}}" pdground="{{pdground}}" icon="{{icon}}" iconSize="{{iconsize}}" lv="{{lv}}" onval="{{onval}}" loadMake="{{loadMake}}" data-ref="qrcode" data-event-opts="{{[['^result',[['qrR']]]]}}" bind:result="__e" bind:__l="__l"></tki-qrcode></view></view>