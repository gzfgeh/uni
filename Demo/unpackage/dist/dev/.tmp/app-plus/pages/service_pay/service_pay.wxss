
.mui-content{
	background-color: #FFFFFF;
	overflow: hidden;
}
.head_wrap{
	margin: 36rpx 30rpx 0rpx;
	padding: 36rpx 0rpx 10rpx 28rpx;
	color: #323333;
	font-size: 28rpx;
	-webkit-box-shadow: 0px 0px 20rpx 0px #EAEAEA;
	        box-shadow: 0px 0px 20rpx 0px #EAEAEA;
