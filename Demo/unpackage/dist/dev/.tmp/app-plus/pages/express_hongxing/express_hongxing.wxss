
.logo_wrap{padding-top: 20rpx;}
.logo_wrap ._img{width: 180rpx;}
.company-modal{width:500rpx;text-align:center;}
.company-code{padding:30rpx 0 30rpx 0;}
.company-code input{border:1px solid #eee;text-align:left;padding:20rpx;}
.pd{padding:0 30rpx;}
.person-express-list{border:2rpx solid rgba(243, 243, 243, 1);
    -webkit-box-shadow:0px 4rpx 12rpx 0px rgba(126,125,125,0.14);
            box-shadow:0px 4rpx 12rpx 0px rgba(126,125,125,0.14);
    border-radius:8rpx;margin:32rpx 0;}
.person-express-list ._li{height:160rpx; width: 90%; padding: 0rpx 40rpx;}
.person-express-list ._li .person-express-detail{border-bottom:2rpx solid #e3e3e3; height: 158rpx;}
/* .person-express-list li .person-express-detail:last-child{border-bottom:0;} */
.person-express-list ._li ._img{width:78rpx; margin-right: 34rpx;}
.person-express-list ._li .right-col{position: absolute;right:20rpx;top:52rpx;}
.person-express-list ._li .right-col ._span{font-size:48rpx;color:#999999;}
.person-express-list ._li .text ._p{color:#999999;font-size:26rpx;padding-top:4rpx;}
.person-express-bottom{padding:0 16rpx;}
.person-express-bottom .person-express-col{width:50%;float:left;-webkit-box-sizing: border-box;box-sizing: border-box;padding:0 16rpx;margin-bottom: 50rpx!important;}
.person-express-bottom .person-express-col .person-express-detail{border:2rpx solid rgba(243, 243, 243, 1);
    -webkit-box-shadow:0px 4rpx 12rpx 0px rgba(126,125,125,0.14);
            box-shadow:0px 4rpx 12rpx 0px rgba(126,125,125,0.14);
    border-radius:8rpx;}
.person-express-bottom .person-express-col .person-express-detail{text-align: center;height:188rpx;-webkit-box-sizing: border-box;box-sizing: border-box;padding-top:32rpx;}
.person-express-bottom .person-express-col .person-express-detail ._div{font-size:26rpx;color:#666666;text-align: center;}
.person-express-bottom .person-express-col .person-express-detail ._img{width:108rpx;}

