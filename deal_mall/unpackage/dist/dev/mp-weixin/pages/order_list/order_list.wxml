<view><view class="head_wrap _div"><block wx:for="{{typeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeType',[index]]]]]}}" class="head_item row_center _div" bindtap="__e"><label class="{{['_span',(curType==index)?'active_type':'']}}">{{item}}</label></view></block></view><view class="list_wrap _div"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.good_list.length>0}}"><view class="list_item _div"><view class="row_between _div"><label class="_span">{{"订单号:"+item.o_out_trade_no}}</label><label class="_span">{{item.o_create_time}}</label></view><block wx:for="{{item.good_list}}" wx:for-item="ite" wx:for-index="ind" wx:key="ind"><view class="item_info row _div"><image src="{{ite.go_g_img}}" mode="aspectFill" class="_img"></image><view class="item_tag flex_one item_content _div"><view class="item_tag flex_one _div"><label class="name _span">{{ite.go_g_name}}</label></view><view class="row _div"><label class="num flex_one _span">{{"x"+ite.go_count}}</label><label class="price _span">{{"￥:"+ite.go_g_price}}</label></view></view></view></block><block wx:if="{{curType>0}}"><view class="row_between express_wrap _div"><label class="_span">{{"快递公司："+item.o_express_name}}</label><label class="_span">{{"快递编号: "+item.o_express_no}}</label></view></block><view class="row _div"><label class="price flex_one _span">{{"合计：￥"+item.o_money}}</label><block wx:if="{{curType==0}}"><label data-event-opts="{{[['tap',[['setExpress',[index]]]]]}}" class="send_goods_btn _span" bindtap="__e">发货</label></block></view></view></block></block></view><block wx:if="{{list.length==0}}"><view class="uni-center-item no_data_wrap _div"><label class="_span">暂无订单信息</label></view></block><block wx:if="{{list.length!=0}}"><view class="uni-tab-bar-loading"><uni-load-more vue-id="1" loadingType="{{loadingType}}" contentText="{{loadingText}}" bind:__l="__l"></uni-load-more></view></block><uni-popup vue-id="2" show="{{type==='middle'}}" position="middle" mode="fixed" data-event-opts="{{[['^hidePopup',[['hidePop']]]]}}" bind:hidePopup="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="company-modal _div"><view class="modal-title _div">设置快递信息</view><view class="content_wrap _div"><view class="company-code _div"><label class="_span">快递公司</label><input type="test" placeholder="输入快递公司" data-event-opts="{{[['input',[['__set_model',['','express_name','$event',[]]]]]]}}" value="{{express_name}}" bindinput="__e"/></view><view class="company-code _div"><label class="_span">快递编号</label><input type="number" placeholder="输入快递编号" data-event-opts="{{[['input',[['__set_model',['','express_no','$event',[]]]]]]}}" value="{{express_no}}" bindinput="__e"/></view></view><button size="mini" data-event-opts="{{[['tap',[['confirmShipping']]]]}}" bindtap="__e">提交</button></view></uni-popup></view>