<view><view class="search_wrap _div"><view class="input_wrap row _div"><image src="https://bay.2donghua.com/web/statics/wxapp/images/icon-search.png" mode="widthFix" class="_img"></image><input type="text" confirm-type="search" placeholder="请输入关键字" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindconfirm="__e" bindinput="__e"/></view><label data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="cancle_btn _span" bindtap="__e">取消</label></view><view class="list_wrap _div"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator data-event-opts="{{[['tap',[['goToDetail',[index]]]]]}}" class="list_item" bindtap="__e"><image src="{{item.g_img}}" mode="aspectFill"></image><view class="item_info _div"><label class="item_info_text _span">{{item.g_name}}</label><view class="row_between _div"><label class="price _span">{{"￥"+item.g_price}}</label><label class="sell_num _span">{{"库存"+item.g_kucun+"件"}}</label></view></view></navigator></block></view><block wx:if="{{list.length==0}}"><view class="uni-center-item no_data_wrap _div"><label class="_span">暂无数据</label></view></block><block wx:if="{{list.length!=0}}"><view class="uni-tab-bar-loading"><uni-load-more vue-id="1" loadingType="{{loadingType}}" contentText="{{loadingText}}" bind:__l="__l"></uni-load-more></view></block></view>