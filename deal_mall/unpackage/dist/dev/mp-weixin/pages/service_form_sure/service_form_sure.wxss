
.mui-content{
				background-color: #FFFFFF;
}
.head_wrap{
				padding: 36rpx 0rem 36rpx 32rpx;
				width: 100%;
				text-align: left;
				color: #333333;
				font-size: 32rpx;
				-webkit-box-sizing: border-box;
				        box-sizing: border-box;
}
.item_wrap{
				width: 100%;
				background-color: #FFFFFF;
				height: 96rpx;
				border-bottom: 2rpx solid #E3E3E3;
				color: #333333;
				font-size: 28rpx;
}
.item_wrap .content{
				color: #666666;
				font-size: 28rpx;
}
.button{
				border-radius: 6rpx;
				width: 90%;
				margin: 28rpx 5%;
				-webkit-box-sizing: border-box;
				        box-sizing: border-box;
				background-color: #107EFF;
				color: #FEFEFE;
				font-size: 32rpx;
				height: 88rpx;
}
.img_wrap{
				width: 92%;
				text-align: center;
				height: 140rpx;
				margin-left: 4%;
				-webkit-box-shadow: 0px 0px 20rpx 0px #EAEAEA;
				        box-shadow: 0px 0px 20rpx 0px #EAEAEA;
				padding: 0rpx 36rpx;
				-webkit-box-sizing: border-box;
				        box-sizing: border-box;
}
.img_wrap ._img{
				width: 40rpx;
				height: 46rpx;
				margin-right: 36rpx;
}
.img_wrap ._span{
				font-size: 28rpx;
				color: #333333;
				font-weight: bold;
				text-align: left;
}
.time_wrap{
				padding: 0rpx 32rpx;
				color: #999999;
				font-size: 28rpx;
				margin: 26rpx 0rpx 46rpx;
}
.content_wrap{
				-webkit-box-shadow: 0px 0px 20rpx 0px #EAEAEA;
				        box-shadow: 0px 0px 20rpx 0px #EAEAEA;
				height: 392rpx;
				padding: 0rpx 26rpx;
				width: 92%;
				margin-left: 4%;
				margin-bottom: 26rpx;
				-webkit-box-sizing: border-box;
				        box-sizing: border-box;
}
.content_wrap .info{
				color: #333333;
				font-size: 32rpx;
				height: 104rpx;
				line-height: 104rpx;
				font-weight: bold;
				border-bottom: 2rpx solid #E3E3E3;
}
.content_wrap .area{
				text-align: left; color:#666666; font-size:28rpx;
				height: 134rpx;
				padding: 32rpx 2rpx 0rpx;
}
.bottom_wrap{
				height: 144rpx;
				padding: 0rpx 30rpx;
				position: fixed;
				bottom: 0rpx;
				left: 0rpx;
				width: 100%;
				-webkit-box-sizing: border-box;
				        box-sizing: border-box;
				background-color: #FFFFFF;
				-webkit-box-shadow: 0px 0px 20rpx 0px #EAEAEA;
				        box-shadow: 0px 0px 20rpx 0px #EAEAEA;
}
.bottom_wrap .btn{
				width: 364rpx;
				height: 88rpx;
				line-height: 88rpx;
				color: #FFFFFF;
				background-color: #107EFF;
				border-radius: 6rpx;
				text-align: center;
}
.money_info{
				color: #333333;
				font-size: 32rpx;
}
.money_num{
				color: #FF5269; font-size: 48rpx;
}

