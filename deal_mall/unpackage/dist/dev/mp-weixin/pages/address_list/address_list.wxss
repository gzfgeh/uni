
page{height: 100%; color: #555; background: #EFEFF4; overflow: hidden;}
.bottom_wrap{position: fixed;bottom: 0rpx;left: 0rpx; width: 100%; z-index: 10; color: #FFF; background-color: #ff4544;height: 100rpx;line-height: 100rpx; text-align: center;}
.list_wrap{padding-bottom: 100rpx;}
.item_wrap{background: #FFF; padding: 32rpx 24rpx; margin-bottom: 20rpx;}
.item_head{margin-bottom: 24rpx;}
.detail{border-bottom: 1rpx solid #EEE; margin-bottom: 24rpx; padding-bottom: 24rpx;}
.edit_item{margin-left: 48rpx;}
.edit_icon{width: 32rpx; height: 32rpx; margin-right: 16rpx;}
.radio_icon{width: 38rpx; height: 38rpx; margin-right: 10rpx;}
.active{color: #fe6e00;}
.empty_text {
	width: 100%;
	height: 800rpx;
	line-height: 800rpx;
	text-align: center;
	background: #f4f4f4;
	border: none;
	z-index: 10;
	background: #f2f2f2;
}


